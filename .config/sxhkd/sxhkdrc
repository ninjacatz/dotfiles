Scroll_Lock
Num_Lock

## KEYBINDS YOU CANT GET RID OF
# ctrl + Alt + F1 - F3 -> tty
# ctrl + Alt + F7 -> exit tty

############ Blue Keys Top Left to Bottom Right
XF86ScreenSaver
	~/.local/userscripts/bin/qqpc -lock
XF86Battery
	~/.local/userscripts/bin/qqcpu -mode start && ~/.local/userscripts/bin/qqcpu -governor conservative
shift + XF86Battery
	~/.local/userscripts/bin/qqcpu -mode ac && ~/.local/userscripts/bin/qqcpu -governor performance
XF86Sleep
	~/.local/userscripts/bin/qqpc -suspend
{_,shift + ,shift + ctrl + }XF86WLAN
	~/.local/userscripts/bin/qqnet -{wifi,ethernet,stop}
XF86WebCam
	guvcview -g none
{_,shift + ,shift + ctrl + }XF86Display
	~/.local/userscripts/bin/qqscreen -{internal,external,internal-external}
# XF86TouchpadToggle:
{_,shift + }XF86Launch3
	~/.local/userscripts/bin/qqdevice -trackpad -{stop,start}
XF86Suspend
	~/.local/userscripts/bin/qqpc -hibernate
{XF86MonBrightnessUp,XF86MonBrightnessDown}
	~/.local/userscripts/bin/qqbacklight -internal -{up,down}
shift + {XF86MonBrightnessUp,XF86MonBrightnessDown}
	~/.local/userscripts/bin/qqbacklight -external -{up,down}
# ThinkLight (Automatic)
# Zoom:
XF86Launch4
	xzoom
{XF86AudioStop,XF86AudioPlay,XF86AudioPrev,XF86AudioNext}
	~/.local/userscripts/bin/qqmpv-controls -{stop,play-pause,rewind,forward}
shift + {XF86AudioPrev,XF86AudioNext}
	~/.local/userscripts/bin/qqmpv-controls -{seek-rewind,seek-forward}

############ Buttons
XF86AudioMute
	~/.local/userscripts/bin/qqvolume -mute -toggle
XF86AudioLowerVolume
	~/.local/userscripts/bin/qqvolume -down
XF86AudioRaiseVolume
	~/.local/userscripts/bin/qqvolume -up
# Mic Mute Button:
XF86Launch1
	~/.local/userscripts/bin/qqvolume -input -toggle
# ThinkVantage Button:
{_,shift + }XF86Launch2
	~/.local/userscripts/bin/qqvolume -output -{default,displayport}
XF86PowerOff
	alacritty --config-file ~/.config/alacritty/script.yml -e ~/.local/userscripts/bin/qqpc -menu

############ 1 -> 0
super + grave
	~/.local/userscripts/bin/qqbspwm -desktop-occupied -next
super + shift+ grave
	~/.local/userscripts/bin/qqbspwm -desktop-occupied -prev
super + {1,2,3,4}
	bspc desktop -f {1,2,3,4}
super + shift + {1,2,3,4}
	bspc node -d {1,2,3,4}
super + shift + ctrl + {1,2,3,4}
	for node in $(bspc query -N -d); do bspc node "$node" -d {1,2,3,4}; done
super + 5
	alacritty --config-file ~/.config/alacritty/vim.yml --class vim -e vim ~/.bashrc ~/.vim/vimrc ~/.config/vifm/vifmrc
super + 6
	alacritty --config-file ~/.config/alacritty/vim.yml --class vim -e vim ~/.config/bspwm/bspwmrc ~/.config/sxhkd/sxhkdrc ~/.config/polybar/config.ini
super + 7
	alacritty --config-file ~/.config/alacritty/vim.yml --class vim -e vim ~/.config/zathura/zathurarc ~/.config/alacritty/alacritty.yml
super + 8
	alacritty --config-file ~/.config/alacritty/vim.yml --class vim -e vim ~/.config/mpv/mpv.conf ~/.config/mpv/input.conf ~/.asoundrc
super + 9
	alacritty --config-file ~/.config/alacritty/vim.yml --class vim -e vim ~/.local/userscripts/bin
super + 0
	alacritty --config-file ~/.config/alacritty/vim.yml --class vim -e sh -c "sudo -E vim /etc/tlp.conf /etc/default/grub /etc/fstab /etc/inputrc /etc/acpi/events/lidbtn /etc/udev/hwdb.d/99-thinkpad-extra.hwdb"
super + minus
super + equal
super + BackSpace
	~/.config/bspwm/bspwmrc

############ Q -> P
super + Tab
super + q
	bspc node -c
super + w
	librewolf
super + shift + w
	dillo
super + {_, shift + }e
	alacritty --config-file ~/.config/alacritty/vim.yml --class vim -e {vim,sudo -E vim}
super + {_, shift + }r
	~/.local/userscripts/bin/qqrecord -{screen-mic,stop}
super + t
	alacritty --config-file ~/.config/alacritty/term.yml
super + shift + t
	alacritty --config-file ~/.config/alacritty/term.yml -e sudo -E bash
super + {_, shift + }y
	~/.local/userscripts/bin/qqnet -ssh -{start,stop}
super + u
super + i
super + o
super + p
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqiphone-sync; sleep 3"
super + bracketleft
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqmount ext1; sleep 3"
super + shift + bracketleft
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqmount -u ext1; sleep 3"
super + bracketright
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqmount ext2; sleep 3"
super + shift + bracketright
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqmount -u ext2; sleep 3"
super + backslash
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqnfs -mount home-server; sleep 3"
super + shift + backslash
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqnfs -umount home-server; sleep 3"

############ A -> L
super + a
super + {_, shift + }s
	~/.local/userscripts/bin/qqrecord -{screenshot,screencrop}
super + d
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqdpi; sleep 3"
super + {_, shift + }f
	alacritty --config-file ~/.config/alacritty/vifm.yml -e {vifm,sudo -E vifm}
super + {_, shift + }g
	~/.local/userscripts/bin/qqnet -mac -{start,stop}
super + {h,j,k,l}
	~/.local/userscripts/bin/qqbspwm -select {west,south,north,east}
super + shift + {h,j,k,l}
	~/.local/userscripts/bin/qqbspwm -move {west,south,north,east}
super + semicolon
	~/.local/userscripts/bin/qqbspwm -select next
super + apostrophe
	~/.local/userscripts/bin/qqbspwm -tiled
super + Return
	~/.local/userscripts/bin/qqbspwm -fullscreen

############ Z -> M
super + z
super + {_, shift + }x
	~/.local/userscripts/bin/qqcoffee -{on,off}
super + c
	alacritty --config-file ~/.config/alacritty/term.yml  -e ~/.local/userscripts/bin/qqcalendar
super + {_, shift + }v
	~/.local/userscripts/bin/qqnet -vpn -{start,stop}
super + b
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqbackup; sleep 3"
super + {_, shift + }n
	alacritty --config-file ~/.config/alacritty/vim.yml --class vim -e ~/.local/userscripts/bin/{qqnotes,qqjournal}
super + m
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqmount; sleep 3"
super + shift + m
	alacritty --config-file ~/.config/alacritty/script.yml -e sh -c "~/.local/userscripts/bin/qqmount -u; sleep 3"
super + comma
super + {_, shift + }period
	~/.local/userscripts/bin/qqbspwm -bar -{hide,show}
super + slash
	~/.local/userscripts/bin/qqbspwm -floating

############ OTHER
super + {_, shift + }space
	~/.local/userscripts/bin/qqrofi -{app,game}
super + {Left,Right,Up,Down}
	~/.local/userscripts/bin/qqbspwm -size {left,right,top,bottom}
super + XF86Back
super + XF86Forward
