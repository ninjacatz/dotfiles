#!/bin/bash
set -eo pipefail
source_dir=$(cd -- "$(dirname -- "$0")" && pwd)
. "$source_dir/../qquser-check"

usage () {
	cat <<EOF
Usage: ${0##*/}
Options:
	on|off
EOF
}

trash ~/.asoundrc || true
cp ~/.local/asus-config/.asoundrc ~/.asoundrc

trash ~/.local/userscripts/qqlib || true
cp ~/.local/asus-config/qqlib ~/.local/userscripts/qqlib
